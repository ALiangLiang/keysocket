{
  "name": "Google Music Media Keys",
  "version": "0.1",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "description": "Control Google Music with your keyboard's media keys",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["http://music.google.com/*"],
      "js": ["keysocket.js"]
    }
  ]
}
